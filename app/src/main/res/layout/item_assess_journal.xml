<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent">


    <TextView


        android:id="@+id/access_item_day"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:background="@drawable/border"
        android:ems="6"
        android:gravity="center_vertical"
        android:text="날짜"
        android:textAlignment="center"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/access_item_rating"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:background="@drawable/border"
        android:ems="6"
        android:gravity="center_vertical"
        android:text="기분"
        android:textAlignment="center"


        app:layout_constraintBottom_toTopOf="@+id/access_item_day"
        app:layout_constraintEnd_toEndOf="@+id/access_item_day"
        app:layout_constraintStart_toStartOf="@+id/access_item_day" />

    <ImageView
        android:id="@+id/access_item_graph"
        android:layout_width="wrap_content"


        android:layout_height="wrap_content"
        android:src="@drawable/graph_5"
        app:layout_constraintBottom_toTopOf="@+id/access_item_rating"
        app:layout_constraintEnd_toEndOf="@+id/access_item_rating"
        app:layout_constraintStart_toStartOf="@+id/access_item_rating" />

</androidx.constraintlayout.widget.ConstraintLayout>